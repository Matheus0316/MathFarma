CREATE TABLE USUARIO(
    id integer PRIMARY KEY NOT NULL,
    nome varchar(60),
    email varchar(70),
    senha varchar(50)
);

CREATE TABLE PRODUTO(
    cod integer PRIMARY KEY NOT NULL,
    nome varchar(20),
    categoria integer,
    estoque integer,
    preco decimal
);

INSERT INTO USUARIO VALUES(1, 'Matheus', 'matheus0316bernardo13@gmail.com', '230006004');

CREATE SEQUENCE USUARIO_SEQ START 2;
CREATE SEQUENCE PROD_SEQ START 1;

INSERT INTO USUARIO (id, nome, email, senha) VALUES (nextval('USUARIO_SEQ'),'abacate', 'abacate123', '1234')